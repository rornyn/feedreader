<tr id="row_<%= feed.id%>">
  <td> <%= feed.url %> </td>
  <td> <%= feed.title || "NA" %>
  <td>
    <%= link_to edit_feed_path(feed), title: "Edit", remote: true do %>
      <i class="fa fa-pencil-square-o"></i>
    <%end %>
    <%= link_to feed_path(feed), title: "Delete", method: :delete, data: {confirm: "Are you sure?"}, remote: true do %>
      <i class="fa fa-trash-o" aria-hidden="true"></i>
    <%end %>
  </td>
</tr>
